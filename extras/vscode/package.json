{
  "name": "silkcircuit-theme",
  "author": "Stefanie Jane <stef@hyperbliss.tech>",
  "license": "MIT",
  "displayName": "SilkCircuit Theme",
  "description": "Electric dreams for VS Code - Neon purples, blazing pinks, and glowing cyan accents",
  "version": "1.0.2",
  "publisher": "hyperb1iss",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e1a2e",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperb1iss/silkcircuit-nvim"
  },
  "bugs": {
    "url": "https://github.com/hyperb1iss/silkcircuit-nvim/issues"
  },
  "homepage": "https://github.com/hyperb1iss/silkcircuit-nvim#readme",
  "private": true,
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark theme",
    "neon",
    "purple",
    "pink",
    "cyberpunk",
    "electric",
    "vibrant",
    "colorful"
  ],
  "contributes": {
    "themes": [
      {
        "label": "SilkCircuit Neon",
        "uiTheme": "vs-dark",
        "path": "./silkcircuit-neon.json"
      },
      {
        "label": "SilkCircuit Vibrant",
        "uiTheme": "vs-dark",
        "path": "./silkcircuit-vibrant.json"
      },
      {
        "label": "SilkCircuit Soft",
        "uiTheme": "vs-dark",
        "path": "./silkcircuit-soft.json"
      },
      {
        "label": "SilkCircuit Glow",
        "uiTheme": "vs-dark",
        "path": "./silkcircuit-glow.json"
      }
    ]
  },
  "scripts": {
    "clean": "rm -f silkcircuit-*.vsix",
    "package": "vsce package --no-dependencies",
    "publish:vsce": "vsce publish --no-dependencies",
    "publish:ovsx": "ovsx publish",
    "publish": "npm run package && npm run publish:vsce && npm run publish:ovsx",
    "version:patch": "vsce publish patch",
    "version:minor": "vsce publish minor",
    "version:major": "vsce publish major"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.0.0",
    "ovsx": "^0.9.2",
    "prettier": "^3.3.3"
  }
}
