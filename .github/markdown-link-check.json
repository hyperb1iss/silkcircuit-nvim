{
  "ignorePatterns": [
    {
      "pattern": "^https://via.placeholder.com"
    },
    {
      "pattern": "^https://github.com/hyperb1iss/silkcircuit-nvim"
    },
    {
      "pattern": "^https://twitter.com"
    },
    {
      "pattern": "^https://marketplace.visualstudio.com"
    }
  ],
  "replacements": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
